<html>

<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @{ 
        if (IsSectionDefined("AddToHead"))
        {
            @RenderSection("AddToHead", required: false)
        }
    }
</head>

<body>
    @RenderBody()
</body>

</html>
